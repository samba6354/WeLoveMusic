</div>
<script type="text/javascript">
document.getElementById('choose').onclick=function(){
  var choosed="";
  choosed=document.getElementById('rating').value;
  document.getElementById('choose-text').innerHTML="Thank you for your feed back given as " + choosed;
}
document.getElementById('submit-button').onclick=function(){
  var firstname= "";
  var lastname= "";
  firstname=document.getElementById('fname').value;
  lastname=document.getElementById('lname').value;
  document.getElementById('signup-text').innerHTML="Thank You " + firstname + " " + lastname + " For Sign up, Please Avoid Errors in All fields";
}
</script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js" integrity="sha384-alpBpkh1PFOepccYVYDB4do5UnbKysX5WZXm3XxPqe5iKTfUKjNkCk9SaVuEZflJ" crossorigin="anonymous"></script>
