      <div style="text-align:center;"><script type="text/javascript" src="http://services.webestools.com/cpt_visitors/54152-4-8.js"></script>
      <br></br>
      <select id="rating" class="selectpicker">
          <option>Excellent</option>
          <option>Great</option>
          <option>Good</option>
          <option>Average</option>
          <option>Bad</option>
        </select>
        <button id="choose" type="submit" class="btn btn-info btn-sm">
          Choose</button><br>
          <p></p>
        <div id="choose-text" class="alert alert-info" role="alert">
        <strong> Hello User Please Choose Your Rating For We Love Music</strong>
        </div>
    </div>
