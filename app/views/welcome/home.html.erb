        <div id="signup" class="row">
          <div id="signup" class="col-md-6 offset-md-6">
            <h1 class="block-title mb-4">
              <span class="text-ribbon text-ribbon-success">
              <span>Sign Up Now</span>
              </span>
            </h1>
            <form action="https://facebook.com/WeLoveTeluguMusic/" target="_blank" method="post">
            <div class="text-ribbon-inputs-split">
              <input id="fname" type="text" class="h4 text-ribbon-input text-ribbon-primary" placeholder="First Name" required>
              <input id="lname" type="text" class="h4 text-ribbon-input text-ribbon-primary" placeholder="Last Name" required>
            </div>
            <input type="email" class="h4 text-ribbon-input text-ribbon-input-half text-ribbon-primary" placeholder="Email" required>
            <button id="submit-button" type="submit" class="btn btn-square btn-primary btn-lg btn-block">
              Sign Up !
            </button>
          </form>
          <p></p>
          <div id="signup-text" style="text-align:center;" class="alert alert-success" role="alert">
          <strong>Hello User Please Fill fieldset for Sign up</strong>
          </div>
        </div>
        </div>
      </div>
    </div>
  <div class="block block-inverse accent-primary" id="footer">
    <div class="container">
      <div class="row text-serif">
        <div class="col-md-2 mb-4">
          <ul class="list-unstyled list-spaced">
            <li><h4>Quick Links</h4></li>
            <li class="text-muted"><em><%= link_to 'Video Songs', video_songs_path %></em></li>
            <li class="text-muted"><em><%= link_to 'Audio Songs', songs_path %></em></li>
            <li class="text-muted"><em><%= link_to 'Lyrics', lyrics_path %></em></li>
